{
  "name": "@vite-electron-builder/preload",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "format": "npx prettier --check \"**/*.{js,mjs,cjs,ts,mts,cts,vue,json}\""
  },
  "files": [
    "dist/**"
  ],
  "types": "./src/index.ts",
  "exports": {
    ".": "./dist/_virtual_browser.mjs",
    "./exposed.mjs": "./dist/exposed.mjs"
  },
  "devDependencies": {
    "vite": "5.4.10",
    "vitest": "2.1.4"
  }
}
