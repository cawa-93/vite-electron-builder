{
  "extends": [
    "config:base",
    ":semanticCommits",
    ":automergeTypes"
  ],
  "labels": [
    "dependencies"
  ],
  "baseBranches": [
    "main"
  ],
  "bumpVersion": "patch",
  "patch": {
    "automerge": true
  },
  "minor": {
    "automerge": true
  },
  "packageRules": [
    {
      "depTypeList": [
        "devDependencies"
      ],
      "semanticCommitType": "build"
    },
    {
      "sourceUrlPrefixes": [
        "https://github.com/vitejs/vite/"
      ],
      "groupName": "Vite monorepo packages"
    },
    {
      "packageNames": [
        "electron-releases"
      ],
      "automerge": true,
      "automergeType": "branch",
      "schedule": "at any time",
      "requiredStatusChecks": null,
      "commitBody": "[skip ci]"
    },
    {
      "packagePatterns": [
        "^@typescript-eslint",
        "^eslint"
      ],
      "automerge": true,
      "automergeType": "branch",
      "schedule": "on the first day of the month",
      "groupName": "eslint"
    }
  ],
  "prHourlyLimit": 5,
  "rangeStrategy": "bump"
}
